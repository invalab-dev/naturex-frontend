{
  "name": "naturex-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "format": "prettier --write \"app/**/*.{ts,tsx}\"  \"components/**/*.{ts,tsx}\" \"hooks/**/*.{ts,tsx}\" \"lib/**/*.{ts,tsx}\"",
    "lint": "eslint",
    "dev": "dotenvx run -f .env.naturex-frontend.local -- next dev --turbopack",
    "build:prod:local": "dotenvx run -f .env.naturex-frontend.local -- next build",
    "build:prod": "dotenvx run -f .env.naturex-frontend.prod.build -- next build",
    "start:prod:local": "dotenvx run -f .env.naturex-frontend.local -- node .next/standalone/server.js",
    "start:prod": "dotenvx run -f .env.naturex-frontend.prod -- node .next/standalone/server.js"
  },
